{
  "kind": "collectionType",
  "collectionName": "venues",
  "info": {
    "singularName": "venue",
    "pluralName": "venues",
    "displayName": "Venue",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "VenueName": {
      "type": "text",
      "required": true,
      "unique": true
    },
    "Capacity": {
      "type": "integer"
    },
    "Rating": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "NumberOfReviews": {
      "type": "biginteger",
      "default": "100"
    },
    "Price": {
      "type": "biginteger"
    },
    "VenueID": {
      "type": "uid",
      "required": true
    },
    "VenueImage": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "VenueDescription": {
      "type": "text"
    }
  }
}
